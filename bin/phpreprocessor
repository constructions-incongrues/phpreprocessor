#!/usr/bin/env php
<?php
use Com\AramisAuto\PHPreprocessor\Command\ApplyCommand;
use Com\AramisAuto\PHPreprocessor\Command\ExtractCommand;
use Symfony\Component\Console\Application;

include_once(__DIR__ . '/../vendor/autoload.php');

$application = new Application();
$application->addCommands(array(
    new ApplyCommand(),
    new ExtractCommand(),
));

$application->run();

